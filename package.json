{
  "name": "tauri-plugin-sql-api",
  "version": "0.1.0",
  "description": "API wrapper for the Tauri Plugin for SQL databases",
  "license": "MIT",
  "module": "webview-dist/index.mjs",
  "browser": "webview-dist/index.min.js",
  "exports": {
    ".": {
      "import": "./webview-dist/index.mjs"
    }
  },
  "types": "webview-dist/index.d.ts",
  "files": [
    "webview-dist"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:esm": "tsup webview-src/index.ts --format='esm' --dts --clean",
    "build:browser": "tsup webview-src/index.ts --format='esm' --minify --clean",
    "lint": "eslint --ext ts \"./webview-src/**/*.ts\"",
    "lint-fix": "eslint --fix --ext ts \"./src/**/*.ts\"",
    "prepublishOnly": "yarn build",
    "pretest": "yarn build"
  },
  "dependencies": {
    "@tauri-apps/api": "1.0.0-beta.8",
    "tslib": "2.3.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-lodash-template": "^0.19.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-security": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "tsup": "^5.11.1",
    "typescript": "^4.5.3"
  },
  "authors": [
    "Tauri Programme within The Commons Conservancy"
  ]
}
